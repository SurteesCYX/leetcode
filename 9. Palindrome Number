class Solution {
    public boolean isPalindrome(int x) {
        if(x<0){return false;}
        int temp=x;
        int result=0;
        while(x/10>0){
            result=result*10+x%10;
            x=x/10;
        }
        result=result*10+x;
        System.out.println(result);
        return result==temp;
        //if(result==x)
    }
}
